<!-- Html for new student  registration success page -->
<!DOCTYPE html>
<html>
    <head>
      <meta charset="UTF-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
       <title>Student registration </title>
       <link rel="stylesheet" href="/styles/css/student-register-success.css">
       <link rel="icon" href="/styles/images/eportal-logo.png">

    </head>
    <body>
        <section class="registration">
          <h2> Student Registration  </h2>  <br>   

          <h3> Your student registration is successful  </h3> 
          <h3>Registration information:</h3> <br>
          <img id="reg-img" src="<%=  photo  ? photo : ''  %>"  alt="Your uploaded photo"> 
          <div class="reg-details">
            <p> <strong >Username:</strong> Your Email  <br> <strong >Matricno:</strong> <%= data.matricno %></p>
                
          </div>
          <h6> <a href="/access/log-in">Log in</a> </h6>
        </section>
        
    </body>
</html>
